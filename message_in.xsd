<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
 targetNamespace="http://www.tribeknopp.com" xmlns="http://www.tribeknopp.com"
 elementFormDefault="qualified">
	<xsd:element name="happening" type="happeningType"/>
